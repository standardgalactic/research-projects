</span><span><span class="hljs-keyword">\dot</span></span><span>{</span><span><span class="hljs-keyword">\Phi</span></span><span>} </span><span><span class="hljs-built_in">&amp;</span></span><span>= </span><span><span class="hljs-keyword">\nabla</span></span><span><span class="hljs-built_in">^</span></span><span>2 </span><span><span class="hljs-keyword">\Phi</span></span><span> - V'(</span><span><span class="hljs-keyword">\Phi</span></span><span>) + </span><span><span class="hljs-keyword">\nabla</span></span><span> </span><span><span class="hljs-keyword">\cdot</span></span><span> </span><span><span class="hljs-keyword">\mathbf</span></span><span>{v} </span><span><span class="hljs-keyword">\\</span></span><span>
</span><span><span class="hljs-keyword">\dot</span></span><span>{</span><span><span class="hljs-keyword">\mathbf</span></span><span>{v}} </span><span><span class="hljs-built_in">&amp;</span></span><span>= </span><span><span class="hljs-keyword">\mathbb</span></span><span>{P} </span><span><span class="hljs-keyword">\left</span></span><span>(
</span><span><span class="hljs-keyword">\lambda</span></span><span> </span><span><span class="hljs-keyword">\nabla</span></span><span>(</span><span><span class="hljs-keyword">\nabla</span></span><span> </span><span><span class="hljs-keyword">\cdot</span></span><span> </span><span><span class="hljs-keyword">\mathbf</span></span><span>{v}) - </span><span><span class="hljs-keyword">\nabla</span></span><span> </span><span><span class="hljs-keyword">\Phi</span></span><span>
