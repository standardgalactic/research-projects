<span class="hljs-string">"""
Curator orchestrator: runs personas iteratively and decides when consensus/confidence is good enough.
Saves the debate transcript and final routing decision.
"""</span>

</span><span><span class="hljs-keyword">import</span></span><span> subprocess
</span><span><span class="hljs-keyword">import</span></span><span> json
</span><span><span class="hljs-keyword">import</span></span><span> sys
</span><span><span class="hljs-keyword">import</span></span><span> os
</span><span><span class="hljs-keyword">import</span></span><span> datetime
</span><span><span class="hljs-keyword">from</span></span><span> typing </span><span><span class="hljs-keyword">import</span></span><span> </span><span><span class="hljs-type">Dict</span></span><span>, </span><span><span class="hljs-type">Any</span></span><span>

</span><span><span class="hljs-comment"># Path to persona router (same directory)</span></span><span>
BASE_DIR = os.path.dirname(__file__)
PERSONA_ROUTER = os.path.join(BASE_DIR, </span><span><span class="hljs-string">"persona_router.py"</span></span><span>)

PERSONAS = [</span><span><span class="hljs-string">"archivist"</span></span><span>, </span><span><span class="hljs-string">"formalist"</span></span><span>, </span><span><span class="hljs-string">"synthesist"</span></span><span>, </span><span><span class="hljs-string">"strategist"</span></span><span>]

</span><span><span class="hljs-comment"># Tunable parameters</span></span><span>
MAX_ROUNDS = </span><span><span class="hljs-built_in">int</span></span><span>(os.environ.get(</span><span><span class="hljs-string">"RAG_MAX_ROUNDS"</span></span><span>, </span><span><span class="hljs-string">"3"</span></span><span>))
CONFIDENCE_THRESHOLD = </span><span><span class="hljs-built_in">float</span></span><span>(os.environ.get(</span><span><span class="hljs-string">"RAG_CONF_THRESH"</span></span><span>, </span><span><span class="hljs-string">"0.6"</span></span><span>))

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">run_persona</span></span><span>(</span><span><span class="hljs-params">persona: <span class="hljs-built_in">str</span></span></span><span>, chunk_text: </span><span><span class="hljs-built_in">str</span></span><span>) -&gt; </span><span><span class="hljs-type">Dict</span></span><span>[</span><span><span class="hljs-built_in">str</span></span><span>, </span><span><span class="hljs-type">Any</span></span><span>]:
    <span class="hljs-string">"""
    Call persona_router.py for one persona and return parsed JSON (or fallback raw).
    """</span>
    proc = subprocess.run(
        [PERSONA_ROUTER, persona, </span><span><span class="hljs-string">"-"</span></span><span>],
        </span><span><span class="hljs-built_in">input</span></span><span>=chunk_text.encode(</span><span><span class="hljs-string">"utf-8"</span></span><span>),
        stdout=subprocess.PIPE,
        stderr=subprocess.PIPE,
    )
    stdout = proc.stdout.decode(</span><span><span class="hljs-string">"utf-8"</span></span><span>, errors=</span><span><span class="hljs-string">"replace"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> stdout:
        stderr = proc.stderr.decode(</span><span><span class="hljs-string">"utf-8"</span></span><span>, errors=</span><span><span class="hljs-string">"replace"</span></span><span>)
        </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"_error"</span></span><span>: </span><span><span class="hljs-string">f"No output. stderr: <span class="hljs-subst">{stderr}</span></span></span><span>"}
    </span><span><span class="hljs-keyword">try</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> json.loads(stdout)
    </span><span><span class="hljs-keyword">except</span></span><span> json.JSONDecodeError:
        </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"_raw_output"</span></span><span>: stdout, </span><span><span class="hljs-string">"_parse_error"</span></span><span>: </span><span><span class="hljs-literal">True</span></span><span>}

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">score_persona_outputs</span></span><span>(</span><span><span class="hljs-params">persona_outputs: <span class="hljs-type">Dict</span></span></span><span>[</span><span><span class="hljs-built_in">str</span></span><span>, </span><span><span class="hljs-type">Dict</span></span><span>]) -&gt; </span><span><span class="hljs-type">Dict</span></span><span>[</span><span><span class="hljs-built_in">str</span></span><span>, </span><span><span class="hljs-built_in">float</span></span><span>]:
    <span class="hljs-string">"""
    Heuristic mapping of persona outputs to normalized weights.
    This function is intentionally conservative; replace or extend with ML models if desired.
    """</span>
    weights = {p: </span><span><span class="hljs-number">0.25</span></span><span> </span><span><span class="hljs-keyword">for</span></span><span> p </span><span><span class="hljs-keyword">in</span></span><span> PERSONAS}  </span><span><span class="hljs-comment"># base equal weighting</span></span><span>

    </span><span><span class="hljs-comment"># Formalist: presence of 'proof_obligations' or 'compression_plan' implies derivability importance</span></span><span>
    f_out = persona_outputs.get(</span><span><span class="hljs-string">"formalist"</span></span><span>, {})
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(f_out, </span><span><span class="hljs-built_in">dict</span></span><span>):
        </span><span><span class="hljs-keyword">if</span></span><span> f_out.get(</span><span><span class="hljs-string">"proof_obligations"</span></span><span>):
            weights[</span><span><span class="hljs-string">"formalist"</span></span><span>] += </span><span><span class="hljs-number">0.25</span></span><span>
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-string">"compression_plan"</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> f_out:
            weights[</span><span><span class="hljs-string">"formalist"</span></span><span>] += </span><span><span class="hljs-number">0.1</span></span><span>

    </span><span><span class="hljs-comment"># Synthesist: fertility_score increases weight (expected 0-10)</span></span><span>
    s_out = persona_outputs.get(</span><span><span class="hljs-string">"synthesist"</span></span><span>, {})
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(s_out, </span><span><span class="hljs-built_in">dict</span></span><span>):
        fs = s_out.get(</span><span><span class="hljs-string">"fertility_score"</span></span><span>)
        </span><span><span class="hljs-keyword">try</span></span><span>:
            </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(fs, (</span><span><span class="hljs-built_in">int</span></span><span>, </span><span><span class="hljs-built_in">float</span></span><span>)):
                weights[</span><span><span class="hljs-string">"synthesist"</span></span><span>] += </span><span><span class="hljs-built_in">min</span></span><span>(</span><span><span class="hljs-number">0.5</span></span><span>, fs / </span><span><span class="hljs-number">10.0</span></span><span>)
        </span><span><span class="hljs-keyword">except</span></span><span> Exception:
            </span><span><span class="hljs-keyword">pass</span></span><span>

    </span><span><span class="hljs-comment"># Strategist: low traction_risk increases strategist</span></span><span>
    st_out = persona_outputs.get(</span><span><span class="hljs-string">"strategist"</span></span><span>, {})
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(st_out, </span><span><span class="hljs-built_in">dict</span></span><span>):
        tr = st_out.get(</span><span><span class="hljs-string">"traction_risk"</span></span><span>)
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(tr, </span><span><span class="hljs-built_in">str</span></span><span>) </span><span><span class="hljs-keyword">and</span></span><span> tr.lower() == </span><span><span class="hljs-string">"low"</span></span><span>:
            weights[</span><span><span class="hljs-string">"strategist"</span></span><span>] += </span><span><span class="hljs-number">0.25</span></span><span>
        </span><span><span class="hljs-comment"># explicit leverage points also increase weight</span></span><span>
        </span><span><span class="hljs-keyword">if</span></span><span> st_out.get(</span><span><span class="hljs-string">"leverage_points"</span></span><span>):
            weights[</span><span><span class="hljs-string">"strategist"</span></span><span>] += </span><span><span class="hljs-number">0.1</span></span><span>

    </span><span><span class="hljs-comment"># Archivist: presence of explicit provenance tags in archive output increases archivist</span></span><span>
    a_out = persona_outputs.get(</span><span><span class="hljs-string">"archivist"</span></span><span>, {})
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(a_out, </span><span><span class="hljs-built_in">dict</span></span><span>):
        </span><span><span class="hljs-keyword">if</span></span><span> a_out.get(</span><span><span class="hljs-string">"tags"</span></span><span>) </span><span><span class="hljs-keyword">or</span></span><span> a_out.get(</span><span><span class="hljs-string">"keep"</span></span><span>):
            weights[</span><span><span class="hljs-string">"archivist"</span></span><span>] += </span><span><span class="hljs-number">0.1</span></span><span>

    </span><span><span class="hljs-comment"># Normalize</span></span><span>
    total = </span><span><span class="hljs-built_in">sum</span></span><span>(weights.values())
    </span><span><span class="hljs-keyword">for</span></span><span> k </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">list</span></span><span>(weights.keys()):
        weights[k] = </span><span><span class="hljs-built_in">round</span></span><span>(</span><span><span class="hljs-built_in">float</span></span><span>(weights[k]) / total, </span><span><span class="hljs-number">3</span></span><span>) </span><span><span class="hljs-keyword">if</span></span><span> total &gt; </span><span><span class="hljs-number">0</span></span><span> </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>

    </span><span><span class="hljs-keyword">return</span></span><span> weights

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">curator_debate_and_decide</span></span><span>(</span><span><span class="hljs-params">chunk_text: <span class="hljs-built_in">str</span></span></span><span>) -&gt; </span><span><span class="hljs-type">Dict</span></span><span>[</span><span><span class="hljs-built_in">str</span></span><span>, </span><span><span class="hljs-type">Any</span></span><span>]:
    <span class="hljs-string">"""
