</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">extract_json</span></span><span>(</span><span><span class="hljs-params">text: <span class="hljs-built_in">str</span></span></span><span>) -&gt; </span><span><span class="hljs-built_in">str</span></span><span>:
    </span><span><span class="hljs-keyword">match</span></span><span> = re.search(</span><span><span class="hljs-string">r"(\{(?:[^{}]|(?1))*\})"</span></span><span>, text, re.DOTALL)
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-keyword">match</span></span><span>.group(</span><span><span class="hljs-number">1</span></span><span>) </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">match</span></span><span> </span><span><span class="hljs-keyword">else</span></span><span> text

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">repair_and_retry</span></span><span>(</span><span><span class="hljs-params">persona: <span class="hljs-built_in">str</span></span></span><span>, chunk_text: </span><span><span class="hljs-built_in">str</span></span><span>, raw_output: </span><span><span class="hljs-built_in">str</span></span><span>, attempts: </span><span><span class="hljs-built_in">int</span></span><span> = </span><span><span class="hljs-number">3</span></span><span>) -&gt; </span><span><span class="hljs-built_in">dict</span></span><span>:
    </span><span><span class="hljs-keyword">for</span></span><span> attempt </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>, attempts + </span><span><span class="hljs-number">1</span></span><span>):
