</span><span><span class="hljs-comment"># Sequential RAG memory builder using Ollama (Granite) and the curator orchestration.</span></span><span>
</span><span><span class="hljs-comment"># Usage: bash rag_memory_builder.sh /path/to/directory_with_txt_files</span></span><span>
</span><span><span class="hljs-built_in">set</span></span><span> -euo pipefail

DIR=</span><span><span class="hljs-string">"<span class="hljs-variable">${1:-.}</span></span></span><span>"
MEM_DIR=</span><span><span class="hljs-string">"<span class="hljs-variable">${DIR%/}</span></span></span><span>/memories"
PROGRESS_FILE=</span><span><span class="hljs-string">"<span class="hljs-variable">${DIR%/}</span></span></span><span>/progress.log"
SUMMARY_FILE=</span><span><span class="hljs-string">"<span class="hljs-variable">${DIR%/}</span></span></span><span>/source-control.txt"

</span><span><span class="hljs-built_in">mkdir</span></span><span> -p </span><span><span class="hljs-string">"<span class="hljs-variable">$MEM_DIR</span></span></span><span>"
</span><span><span class="hljs-built_in">touch</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$PROGRESS_FILE</span></span></span><span>"
</span><span><span class="hljs-built_in">touch</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$SUMMARY_FILE</span></span></span><span>"

</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"Starting RAG memory builder at <span class="hljs-subst">$(date -u)</span></span></span><span>" &gt;&gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$PROGRESS_FILE</span></span></span><span>"

</span><span><span class="hljs-comment"># For each top-level .txt file (does not descend into subdirs in this script)</span></span><span>
</span><span><span class="hljs-keyword">for</span></span><span> file </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$DIR</span></span></span><span>"/*.txt; </span><span><span class="hljs-keyword">do</span></span><span>
  [ -f </span><span><span class="hljs-string">"<span class="hljs-variable">$file</span></span></span><span>" ] || </span><span><span class="hljs-built_in">continue</span></span><span>
  base=$(</span><span><span class="hljs-built_in">basename</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$file</span></span></span><span>" .txt)

  </span><span><span class="hljs-comment"># skip if processed</span></span><span>
  </span><span><span class="hljs-keyword">if</span></span><span> grep -Fxq </span><span><span class="hljs-string">"<span class="hljs-variable">$base</span></span></span><span>" </span><span><span class="hljs-string">"<span class="hljs-variable">$PROGRESS_FILE</span></span></span><span>" 2&gt;/dev/null; </span><span><span class="hljs-keyword">then</span></span><span>
    </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"Skipping already processed <span class="hljs-variable">$file</span></span></span><span>" &gt;&gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$PROGRESS_FILE</span></span></span><span>"
    </span><span><span class="hljs-built_in">continue</span></span><span>
  </span><span><span class="hljs-keyword">fi</span></span><span>

  </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"Processing <span class="hljs-variable">$file</span></span></span><span>" &gt;&gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$PROGRESS_FILE</span></span></span><span>"

  </span><span><span class="hljs-comment"># split into chunks of 282 lines</span></span><span>
  tmpdir=$(</span><span><span class="hljs-built_in">mktemp</span></span><span> -d)
  </span><span><span class="hljs-built_in">split</span></span><span> -l 282 </span><span><span class="hljs-string">"<span class="hljs-variable">$file</span></span></span><span>" </span><span><span class="hljs-string">"<span class="hljs-variable">$tmpdir</span></span></span><span>/chunk_"
  idx=0

  </span><span><span class="hljs-keyword">for</span></span><span> chunk </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$tmpdir</span></span></span><span>"/chunk_*; </span><span><span class="hljs-keyword">do</span></span><span>
    idx=$((idx + </span><span><span class="hljs-number">1</span></span><span>))
    memfile=</span><span><span class="hljs-string">"<span class="hljs-variable">${MEM_DIR}</span></span></span><span>/</span><span><span class="hljs-variable">${base}</span></span><span>-</span><span><span class="hljs-variable">${idx}</span></span><span>.memory"
    </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"Processing chunk <span class="hljs-variable">$chunk</span></span></span><span> -&gt; </span><span><span class="hljs-variable">$memfile</span></span><span>" &gt;&gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$PROGRESS_FILE</span></span></span><span>"

    </span><span><span class="hljs-comment"># 1) Human-readable summary appended to source-control.txt</span></span><span>
    </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"### <span class="hljs-variable">$base</span></span></span><span> - chunk </span><span><span class="hljs-variable">$idx</span></span><span>" &gt;&gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$SUMMARY_FILE</span></span></span><span>"
    </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">""</span></span><span> &gt;&gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$SUMMARY_FILE</span></span></span><span>"
    </span><span><span class="hljs-comment"># simple summary call (replace prompt if you prefer)</span></span><span>
    ollama run granite3.2:8b </span><span><span class="hljs-string">"Summarize in detail and explain the main points:"</span></span><span> &lt; </span><span><span class="hljs-string">"<span class="hljs-variable">$chunk</span></span></span><span>" | </span><span><span class="hljs-built_in">tee</span></span><span> -a </span><span><span class="hljs-string">"<span class="hljs-variable">$SUMMARY_FILE</span></span></span><span>"
    </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">""</span></span><span> &gt;&gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$SUMMARY_FILE</span></span></span><span>"

    </span><span><span class="hljs-comment"># 2) JSON summary (one-line JSON) — fail-safe: capture raw output</span></span><span>
    summary_json=$(ollama run granite3.2:8b </span><span><span class="hljs-string">"Summarize in one paragraph and return JSON exactly like {\"summary\":\"...\"}. Return only JSON."</span></span><span> &lt; </span><span><span class="hljs-string">"<span class="hljs-variable">$chunk</span></span></span><span>" || </span><span><span class="hljs-literal">true</span></span><span>)
    </span><span><span class="hljs-comment"># If parsing fails later we keep empty summary_text fallback</span></span><span>
    summary_text=</span><span><span class="hljs-string">""</span></span><span>
    </span><span><span class="hljs-comment"># attempt to extract "summary" if JSON-like</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$summary_json</span></span></span><span>" | jq -e . &gt;/dev/null 2&gt;&amp;1; </span><span><span class="hljs-keyword">then</span></span><span>
      summary_text=$(</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$summary_json</span></span></span><span>" | jq -r </span><span><span class="hljs-string">'.summary // ""'</span></span><span>)
    </span><span><span class="hljs-keyword">else</span></span><span>
      </span><span><span class="hljs-comment"># fallback: use the raw output as summary_text</span></span><span>
      summary_text=$(</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$summary_json</span></span></span><span>" | </span><span><span class="hljs-built_in">head</span></span><span> -n 10)
    </span><span><span class="hljs-keyword">fi</span></span><span>

    </span><span><span class="hljs-comment"># 3) Run curator orchestration (this runs persona_router.py internally)</span></span><span>
    </span><span><span class="hljs-comment"># curator.py accepts chunk text via stdin and prints JSON</span></span><span>
    curator_output=$(python3 </span><span><span class="hljs-string">"<span class="hljs-subst">$(dirname <span class="hljs-string">"<span class="hljs-variable">$0</span></span></span></span></span><span>")/curator.py" </span><span><span class="hljs-string">"-"</span></span><span> &lt; </span><span><span class="hljs-string">"<span class="hljs-variable">$chunk</span></span></span><span>")

    </span><span><span class="hljs-comment"># 4) Build .memory JSON file including raw text, summary, curator outputs</span></span><span>
    created_at=$(</span><span><span class="hljs-built_in">date</span></span><span> -u +</span><span><span class="hljs-string">"%Y-%m-%dT%H:%M:%SZ"</span></span><span>)
    uuid=$(python3 - &lt;&lt;</span><span><span class="hljs-string">'PY'</span></span><span>
import uuid,sys
</span><span><span class="hljs-built_in">print</span></span><span>(str(uuid.uuid4()))
PY
)

    </span><span><span class="hljs-comment"># read chunk content safely into JSON value</span></span><span>
    chunk_json=$(python3 - &lt;&lt;</span><span><span class="hljs-string">'PY'</span></span><span>
import json,sys
txt=sys.stdin.read()
</span><span><span class="hljs-built_in">print</span></span><span>(json.dumps(txt))
PY
&lt;&lt;&lt; </span><span><span class="hljs-string">"<span class="hljs-subst">$(cat <span class="hljs-string">"<span class="hljs-variable">$chunk</span></span></span></span></span><span>")")

    </span><span><span class="hljs-comment"># write memory file</span></span><span>
    </span><span><span class="hljs-built_in">cat</span></span><span> &gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$memfile</span></span></span><span>" &lt;&lt;<span class="hljs-string">EOF
{
  "memory_id": "${uuid}",
  "chunk_source": "${base}.txt",
  "chunk_index": ${idx},
  "text": ${chunk_json},
  "summary": $(python3 - &lt;&lt;'PY'
import json,sys
s=sys.stdin.read()
s=s.strip()
if s=="":
    print(json.dumps(""))
else:
    # if s already JSON, print field safely
