                    </span><span><span class="hljs-string">"debate_log"</span></span><span>:log,</span><span><span class="hljs-string">"persona_outputs"</span></span><span>:outputs}
        log += </span><span><span class="hljs-string">"Retrying...\n"</span></span><span>
    w = score(outputs)
    </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"decision"</span></span><span>:{</span><span><span class="hljs-string">"primary_owner"</span></span><span>:</span><span><span class="hljs-built_in">max</span></span><span>(w,key=w.get),</span><span><span class="hljs-string">"weights"</span></span><span>:w,</span><span><span class="hljs-string">"rounds"</span></span><span>:MAX_ROUNDS},
