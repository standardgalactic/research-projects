    Returns dict with: decision (primary_owner, weights, rounds), debate_log, persona_outputs
    """</span>
    debate_lines = []
    persona_outputs = {}

    </span><span><span class="hljs-keyword">for</span></span><span> r </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>, MAX_ROUNDS + </span><span><span class="hljs-number">1</span></span><span>):
        debate_lines.append(</span><span><span class="hljs-string">f"=== ROUND <span class="hljs-subst">{r}</span></span></span><span> — </span><span><span class="hljs-subst">{datetime.datetime.utcnow().isoformat()}</span></span><span> ===\n")
        </span><span><span class="hljs-comment"># sequentially run each persona</span></span><span>
        </span><span><span class="hljs-keyword">for</span></span><span> p </span><span><span class="hljs-keyword">in</span></span><span> PERSONAS:
            debate_lines.append(</span><span><span class="hljs-string">f"-- <span class="hljs-subst">{p.upper()}</span></span></span><span> START --\n")
            out = run_persona(p, chunk_text)
            persona_outputs[p] = out
            </span><span><span class="hljs-comment"># store pretty-printed JSON in the log</span></span><span>
            </span><span><span class="hljs-keyword">try</span></span><span>:
                debate_lines.append(json.dumps(out, indent=</span><span><span class="hljs-number">2</span></span><span>, ensure_ascii=</span><span><span class="hljs-literal">False</span></span><span>) + </span><span><span class="hljs-string">"\n"</span></span><span>)
            </span><span><span class="hljs-keyword">except</span></span><span> Exception:
                debate_lines.append(</span><span><span class="hljs-built_in">repr</span></span><span>(out) + </span><span><span class="hljs-string">"\n"</span></span><span>)
            debate_lines.append(</span><span><span class="hljs-string">f"-- <span class="hljs-subst">{p.upper()}</span></span></span><span> END --\n\n")

        </span><span><span class="hljs-comment"># score</span></span><span>
        weights = score_persona_outputs(persona_outputs)
        top_persona = </span><span><span class="hljs-built_in">max</span></span><span>(weights, key=</span><span><span class="hljs-keyword">lambda</span></span><span> k: weights[k])
        top_weight = weights[top_persona]
        debate_lines.append(</span><span><span class="hljs-string">f"Round <span class="hljs-subst">{r}</span></span></span><span> weights: </span><span><span class="hljs-subst">{json.dumps(weights)}</span></span><span>\n")

        </span><span><span class="hljs-keyword">if</span></span><span> top_weight &gt;= CONFIDENCE_THRESHOLD:
            decision = {</span><span><span class="hljs-string">"primary_owner"</span></span><span>: top_persona, </span><span><span class="hljs-string">"weights"</span></span><span>: weights, </span><span><span class="hljs-string">"rounds"</span></span><span>: r}
            </span><span><span class="hljs-keyword">return</span></span><span> {
                </span><span><span class="hljs-string">"decision"</span></span><span>: decision,
                </span><span><span class="hljs-string">"debate_log"</span></span><span>: </span><span><span class="hljs-string">""</span></span><span>.join(debate_lines),
                </span><span><span class="hljs-string">"persona_outputs"</span></span><span>: persona_outputs,
            }

        </span><span><span class="hljs-comment"># If not confident, optionally modify or add context; this simple implementation proceeds to next round.</span></span><span>
        debate_lines.append(</span><span><span class="hljs-string">"Not confident enough, continuing to next round.\n"</span></span><span>)

    </span><span><span class="hljs-comment"># fallback after MAX_ROUNDS: choose top persona anyway</span></span><span>
    weights = score_persona_outputs(persona_outputs)
    top_persona = </span><span><span class="hljs-built_in">max</span></span><span>(weights, key=</span><span><span class="hljs-keyword">lambda</span></span><span> k: weights[k])
    </span><span><span class="hljs-keyword">return</span></span><span> {
        </span><span><span class="hljs-string">"decision"</span></span><span>: {</span><span><span class="hljs-string">"primary_owner"</span></span><span>: top_persona, </span><span><span class="hljs-string">"weights"</span></span><span>: weights, </span><span><span class="hljs-string">"rounds"</span></span><span>: MAX_ROUNDS},
        </span><span><span class="hljs-string">"debate_log"</span></span><span>: </span><span><span class="hljs-string">""</span></span><span>.join(debate_lines),
        </span><span><span class="hljs-string">"persona_outputs"</span></span><span>: persona_outputs,
    }

</span><span><span class="hljs-comment"># CLI: curator.py &lt;chunk_file_or_-&gt;</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">len</span></span><span>(sys.argv) &lt; </span><span><span class="hljs-number">2</span></span><span>:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"Usage: curator.py &lt;chunk_file_or_-&gt;"</span></span><span>, file=sys.stderr)
        sys.exit(</span><span><span class="hljs-number">2</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> sys.argv[</span><span><span class="hljs-number">1</span></span><span>] == </span><span><span class="hljs-string">"-"</span></span><span>:
        chunk_text = sys.stdin.read()
    </span><span><span class="hljs-keyword">else</span></span><span>:
