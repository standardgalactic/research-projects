    </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"_raw_output"</span></span><span>: raw_output, </span><span><span class="hljs-string">"_repair_failed"</span></span><span>: </span><span><span class="hljs-literal">True</span></span><span>}

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">analyze_with_persona</span></span><span>(</span><span><span class="hljs-params">persona: <span class="hljs-built_in">str</span></span></span><span>, chunk_text: </span><span><span class="hljs-built_in">str</span></span><span>) -&gt; </span><span><span class="hljs-built_in">dict</span></span><span>:
