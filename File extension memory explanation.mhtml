    tot = </span><span><span class="hljs-built_in">sum</span></span><span>(w.values())
    </span><span><span class="hljs-keyword">return</span></span><span> {k:</span><span><span class="hljs-built_in">round</span></span><span>(v/tot,</span><span><span class="hljs-number">3</span></span><span>) </span><span><span class="hljs-keyword">for</span></span><span> k,v </span><span><span class="hljs-keyword">in</span></span><span> w.items()}

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">curator_debate_and_decide</span></span><span>(</span><span><span class="hljs-params">text</span></span><span>):
