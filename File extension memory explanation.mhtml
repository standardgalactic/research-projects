<span class="hljs-string">"""
Persona router: constructs persona prompts, calls Ollama synchronously, and returns structured JSON results.
This module shells out to 'ollama run &lt;model&gt;' for local LLM calls.
Adjust the model tag to match your installed Ollama model (e.g. granite3.2:8b).
"""</span>

</span><span><span class="hljs-keyword">import</span></span><span> subprocess
</span><span><span class="hljs-keyword">import</span></span><span> json
</span><span><span class="hljs-keyword">import</span></span><span> shlex
</span><span><span class="hljs-keyword">import</span></span><span> sys
</span><span><span class="hljs-keyword">import</span></span><span> os

</span><span><span class="hljs-comment"># === CONFIG ===</span></span><span>
OLLAMA_MODEL = os.environ.get(</span><span><span class="hljs-string">"OLLAMA_MODEL"</span></span><span>, </span><span><span class="hljs-string">"granite3.2:8b"</span></span><span>)  </span><span><span class="hljs-comment"># change if needed</span></span><span>

PERSONA_PROMPTS = {
    </span><span><span class="hljs-string">"archivist"</span></span><span>: (
        </span><span><span class="hljs-string">"You are the Archivist (RAG-01). Your job is: preserve optionality, "</span></span><span>
        </span><span><span class="hljs-string">"record provenance and lineage, and highlight items 'worth keeping' for "</span></span><span>
        </span><span><span class="hljs-string">"future, uncertain scenarios. Respond *strictly* with JSON. Keys: "</span></span><span>
        </span><span><span class="hljs-string">"keep (list of items to keep), delete (list of items safe to drop), "</span></span><span>
        </span><span><span class="hljs-string">"tags (list), rationale (short string)."</span></span><span>
    ),
    </span><span><span class="hljs-string">"formalist"</span></span><span>: (
        </span><span><span class="hljs-string">"You are the Formalist (RAG-02). Your job is: identify axioms, "</span></span><span>
        </span><span><span class="hljs-string">"definitions, derivations, and proof obligations necessary to reconstruct "</span></span><span>
        </span><span><span class="hljs-string">"the content. Respond *strictly* with JSON. Keys: keep, delete, "</span></span><span>
        </span><span><span class="hljs-string">"compression_plan (string), proof_obligations (list), rationale."</span></span><span>
    ),
    </span><span><span class="hljs-string">"synthesist"</span></span><span>: (
        </span><span><span class="hljs-string">"You are the Synthesist (RAG-03). Your job is: find bridges, analogies, "</span></span><span>
        </span><span><span class="hljs-string">"open questions and recombination opportunities. Respond *strictly* "</span></span><span>
        </span><span><span class="hljs-string">"with JSON. Keys: keep, delete, branches (list of 'what if' ideas), "</span></span><span>
        </span><span><span class="hljs-string">"fertility_score (number 0-10), rationale."</span></span><span>
    ),
    </span><span><span class="hljs-string">"strategist"</span></span><span>: (
        </span><span><span class="hljs-string">"You are the Strategist (RAG-04). Your job is: identify levers, bottlenecks, "</span></span><span>
        </span><span><span class="hljs-string">"adoption blockers, and next-actions to convert this into deployed value. "</span></span><span>
        </span><span><span class="hljs-string">"Respond *strictly* with JSON. Keys: keep, delete, leverage_points (list), "</span></span><span>
        </span><span><span class="hljs-string">"next_action (string), traction_risk (one of low/medium/high), rationale."</span></span><span>
    },
}

</span><span><span class="hljs-comment"># === UTIL ===</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">call_ollama</span></span><span>(</span><span><span class="hljs-params">prompt: <span class="hljs-built_in">str</span></span></span><span>, stdin_text: </span><span><span class="hljs-built_in">str</span></span><span>) -&gt; </span><span><span class="hljs-built_in">str</span></span><span>:
    <span class="hljs-string">"""
    Call Ollama CLI and return stdout as string.
    Note: blocking, sequential call.
    """</span>
    model = OLLAMA_MODEL
    </span><span><span class="hljs-comment"># Use shlex.quote only for prompt wrapper; we pass stdin for chunk text.</span></span><span>
    cmd = </span><span><span class="hljs-string">f"ollama run <span class="hljs-subst">{shlex.quote(model)}</span></span></span><span> </span><span><span class="hljs-subst">{shlex.quote(prompt)}</span></span><span>"
    </span><span><span class="hljs-keyword">try</span></span><span>:
        proc = subprocess.run(
            cmd,
            </span><span><span class="hljs-built_in">input</span></span><span>=stdin_text.encode(</span><span><span class="hljs-string">"utf-8"</span></span><span>),
            shell=</span><span><span class="hljs-literal">True</span></span><span>,
            check=</span><span><span class="hljs-literal">True</span></span><span>,
            stdout=subprocess.PIPE,
            stderr=subprocess.PIPE,
        )
        </span><span><span class="hljs-keyword">return</span></span><span> proc.stdout.decode(</span><span><span class="hljs-string">"utf-8"</span></span><span>, errors=</span><span><span class="hljs-string">"replace"</span></span><span>)
    </span><span><span class="hljs-keyword">except</span></span><span> subprocess.CalledProcessError </span><span><span class="hljs-keyword">as</span></span><span> e:
        stderr = e.stderr.decode(</span><span><span class="hljs-string">"utf-8"</span></span><span>, errors=</span><span><span class="hljs-string">"replace"</span></span><span>) </span><span><span class="hljs-keyword">if</span></span><span> e.stderr </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-string">""</span></span><span>
        </span><span><span class="hljs-keyword">raise</span></span><span> RuntimeError(</span><span><span class="hljs-string">f"Ollama call failed (rc=<span class="hljs-subst">{e.returncode}</span></span></span><span>): </span><span><span class="hljs-subst">{stderr}</span></span><span>") </span><span><span class="hljs-keyword">from</span></span><span> e

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">analyze_with_persona</span></span><span>(</span><span><span class="hljs-params">persona: <span class="hljs-built_in">str</span></span></span><span>, chunk_text: </span><span><span class="hljs-built_in">str</span></span><span>) -&gt; </span><span><span class="hljs-built_in">dict</span></span><span>:
    <span class="hljs-string">"""
    Run a single persona prompt against a text chunk.
    Returns a dict parsed from the model stdout if JSON; otherwise returns raw output wrapped.
    """</span>
    </span><span><span class="hljs-keyword">if</span></span><span> persona </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> PERSONA_PROMPTS:
        </span><span><span class="hljs-keyword">raise</span></span><span> ValueError(</span><span><span class="hljs-string">f"Unknown persona: <span class="hljs-subst">{persona}</span></span></span><span>")
    prompt = PERSONA_PROMPTS[persona] + </span><span><span class="hljs-string">"\n\nMemory chunk (do not include explanations beyond the requested JSON):\n"</span></span><span>
    </span><span><span class="hljs-comment"># We append a short instruction requiring JSON only to reduce parsing failures:</span></span><span>
    prompt += </span><span><span class="hljs-string">"\nReturn only a single JSON object (no surrounding text)."</span></span><span>
    out = call_ollama(prompt, chunk_text)
    </span><span><span class="hljs-comment"># Try JSON parse:</span></span><span>
    stripped = out.strip()
    </span><span><span class="hljs-keyword">try</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> json.loads(stripped)
    </span><span><span class="hljs-keyword">except</span></span><span> json.JSONDecodeError:
        </span><span><span class="hljs-comment"># Return parsing error along with raw text so curator can decide/retry</span></span><span>
        </span><span><span class="hljs-keyword">return</span></span><span> {</span><span><span class="hljs-string">"_raw_output"</span></span><span>: stripped, </span><span><span class="hljs-string">"_parse_error"</span></span><span>: </span><span><span class="hljs-literal">True</span></span><span>}

</span><span><span class="hljs-comment"># CLI usage: persona_router.py &lt;persona&gt; &lt;chunk_file_or_-&gt;</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">len</span></span><span>(sys.argv) &lt; </span><span><span class="hljs-number">3</span></span><span>:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"Usage: persona_router.py &lt;persona&gt; &lt;chunk_file_or_-&gt;"</span></span><span>, file=sys.stderr)
        sys.exit(</span><span><span class="hljs-number">2</span></span><span>)
    persona = sys.argv[</span><span><span class="hljs-number">1</span></span><span>]
    target = sys.argv[</span><span><span class="hljs-number">2</span></span><span>]
    </span><span><span class="hljs-keyword">if</span></span><span> target == </span><span><span class="hljs-string">"-"</span></span><span>:
        chunk_text = sys.stdin.read()
    </span><span><span class="hljs-keyword">else</span></span><span>:
        </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(target, </span><span><span class="hljs-string">"r"</span></span><span>, encoding=</span><span><span class="hljs-string">"utf-8"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
            chunk_text = f.read()
    res = analyze_with_persona(persona, chunk_text)
    </span><span><span class="hljs-built_in">print</span></span><span>(json.dumps(res, indent=</span><span><span class="hljs-number">2</span></span><span>, ensure_ascii=</span><span><span class="hljs-literal">False</span></span><span>))
</span></span></code></div></div></pre>
