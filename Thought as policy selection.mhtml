    </span><span><span class="hljs-comment"># approximate gradient of each basis φ_j at xi</span></span><span>
    row = []
    </span><span><span class="hljs-keyword">for</span></span><span> j </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(N):
        xj = centers[j]
        r = xi - xj
        norm = np.linalg.norm(r) + eps
        grad_phi = r / (norm**</span><span><span class="hljs-number">3</span></span><span>)  </span><span><span class="hljs-comment"># gradient of thin-plate spline kernel</span></span><span>
