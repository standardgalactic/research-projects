</span><span><span class="hljs-keyword">from</span></span><span> matplotlib.patches </span><span><span class="hljs-keyword">import</span></span><span> Ellipse
</span><span><span class="hljs-keyword">import</span></span><span> matplotlib.pyplot </span><span><span class="hljs-keyword">as</span></span><span> plt

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">plot_uncertainty_ellipses</span></span><span>(</span><span><span class="hljs-params">centers, diffusion, ax=<span class="hljs-literal">None</span></span></span><span>, scale=</span><span><span class="hljs-number">2.0</span></span><span>):
    <span class="hljs-string">"""
    centers:   (N, 2)
    diffusion: (N, 2, 2) local diffusion matrices
    scale:     confidence scale (2 ≈ 95%)
    """</span>
    </span><span><span class="hljs-keyword">if</span></span><span> ax </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
        fig, ax = plt.subplots(figsize=(</span><span><span class="hljs-number">5</span></span><span>,</span><span><span class="hljs-number">5</span></span><span>))

    </span><span><span class="hljs-keyword">for</span></span><span> x, D </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(centers, diffusion):
        vals, vecs = np.linalg.eigh(D)
        order = vals.argsort()[::-</span><span><span class="hljs-number">1</span></span><span>]
        vals, vecs = vals[order], vecs[:, order]

        </span><span><span class="hljs-comment"># 2σ ellipse</span></span><span>
        width, height = </span><span><span class="hljs-number">2</span></span><span> * scale * np.sqrt(vals + </span><span><span class="hljs-number">1e-9</span></span><span>)
        angle = np.degrees(np.arctan2(*vecs[:, </span><span><span class="hljs-number">0</span></span><span>][::-</span><span><span class="hljs-number">1</span></span><span>]))

        ell = Ellipse(xy=x, width=width, height=height, angle=angle,
                      facecolor=</span><span><span class="hljs-string">'none'</span></span><span>, edgecolor=</span><span><span class="hljs-string">'black'</span></span><span>, alpha=</span><span><span class="hljs-number">0.3</span></span><span>, lw=</span><span><span class="hljs-number">0.8</span></span><span>)
