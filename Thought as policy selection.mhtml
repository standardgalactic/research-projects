</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">export_3d_potential_html</span></span><span>(</span><span><span class="hljs-params">X1, X2, U, centers, drift, diff, filename=<span class="hljs-string">"potential3d.html"</span></span></span><span>):
    </span><span><span class="hljs-comment"># Surface</span></span><span>
    fig = go.Figure(go.Surface(
        x=X1, y=X2, z=U,
        colorscale=</span><span><span class="hljs-string">'Viridis'</span></span><span>, opacity=</span><span><span class="hljs-number">0.8</span></span><span>, name=</span><span><span class="hljs-string">"U(x)"</span></span><span>
    ))

    </span><span><span class="hljs-comment"># Drift vectors</span></span><span>
    fig.add_trace(go.Cone(
        x=centers[:,</span><span><span class="hljs-number">0</span></span><span>], y=centers[:,</span><span><span class="hljs-number">1</span></span><span>], z=U.ravel()[:</span><span><span class="hljs-built_in">len</span></span><span>(centers)],
        u=drift[:,</span><span><span class="hljs-number">0</span></span><span>], v=drift[:,</span><span><span class="hljs-number">1</span></span><span>], w=np.zeros(</span><span><span class="hljs-built_in">len</span></span><span>(centers)),
        sizemode=</span><span><span class="hljs-string">"absolute"</span></span><span>, sizeref=</span><span><span class="hljs-number">0.4</span></span><span>, anchor=</span><span><span class="hljs-string">"tail"</span></span><span>,
        name=</span><span><span class="hljs-string">"Drift −∇U"</span></span><span>
    ))

    </span><span><span class="hljs-comment"># Uncertainty ellipses as scaled covariance discs (flat 3D)</span></span><span>
    </span><span><span class="hljs-keyword">for</span></span><span> (cx, cy), D </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(centers[:,:</span><span><span class="hljs-number">2</span></span><span>], diff[:,:</span><span><span class="hljs-number">2</span></span><span>,:</span><span><span class="hljs-number">2</span></span><span>]):
        vals, vecs = np.linalg.eigh(D)
        r = </span><span><span class="hljs-number">0.5</span></span><span> * np.sqrt(vals.</span><span><span class="hljs-built_in">max</span></span><span>() + </span><span><span class="hljs-number">1e-9</span></span><span>)
        fig.add_trace(go.Scatter3d(
            x=[cx], y=[cy], z=[</span><span><span class="hljs-number">0</span></span><span>],
            mode=</span><span><span class="hljs-string">'markers'</span></span><span>,
            marker=</span><span><span class="hljs-built_in">dict</span></span><span>(size=r*</span><span><span class="hljs-number">20</span></span><span>, opacity=</span><span><span class="hljs-number">0.3</span></span><span>, color=</span><span><span class="hljs-string">'black'</span></span><span>),
            showlegend=</span><span><span class="hljs-literal">False</span></span><span>
        ))

    fig.update_layout(
        title=</span><span><span class="hljs-string">"RSVP Cognitive Potential Landscape"</span></span><span>,
        scene=</span><span><span class="hljs-built_in">dict</span></span><span>(
            xaxis_title=</span><span><span class="hljs-string">"x1"</span></span><span>,
            yaxis_title=</span><span><span class="hljs-string">"x2"</span></span><span>,
            zaxis_title=</span><span><span class="hljs-string">"U(x)"</span></span><span>
        )
    )

    fig.write_html(filename)
    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"✅ Saved 3D potential to <span class="hljs-subst">{filename}</span></span></span><span>")
</span></span></code></div></div></pre>
