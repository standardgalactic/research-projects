</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">generate_plenum_trajectories</span></span><span>(</span><span><span class="hljs-params">T=<span class="hljs-number">2000</span></span></span><span>, dt=</span><span><span class="hljs-number">0.01</span></span><span>, n_modes=</span><span><span class="hljs-number">5</span></span><span>, decay=</span><span><span class="hljs-number">0.05</span></span><span>, noise_scale=</span><span><span class="hljs-number">0.1</span></span><span>):
    <span class="hljs-string">"""
    Returns latent X shape (n_modes, T)
    Linear AR(1) with coupling via diagonal decay and correlated noise
    """</span>
    T = </span><span><span class="hljs-built_in">int</span></span><span>(T)
    X = np.zeros((n_modes, T))
    A = np.eye(n_modes) * (</span><span><span class="hljs-number">1</span></span><span> - decay)
    </span><span><span class="hljs-comment"># add cross-mode leak to emulate overlap</span></span><span>
    leak = </span><span><span class="hljs-number">0.02</span></span><span>
    A += leak * (np.ones((n_modes,n_modes)) - np.eye(n_modes))
    cov = np.eye(n_modes) * noise_scale
    </span><span><span class="hljs-keyword">for</span></span><span> t </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>, T):
        X[:,t] = A.dot(X[:,t-</span><span><span class="hljs-number">1</span></span><span>]) + np.random.multivariate_normal(np.zeros(n_modes), cov)
    </span><span><span class="hljs-keyword">return</span></span><span> X

