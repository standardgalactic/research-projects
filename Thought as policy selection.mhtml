        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">" "</span></span><span>.join(</span><span><span class="hljs-string">f"(<span class="hljs-subst">{fmt % a}</span></span></span><span>,</span><span><span class="hljs-subst">{fmt % b}</span></span><span>)" </span><span><span class="hljs-keyword">for</span></span><span> a,b </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(x,y))
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">" "</span></span><span>.join(</span><span><span class="hljs-string">f"(<span class="hljs-subst">{fmt % a}</span></span></span><span>,</span><span><span class="hljs-subst">{fmt % b}</span></span><span>,</span><span><span class="hljs-subst">{fmt % c}</span></span><span>)" </span><span><span class="hljs-keyword">for</span></span><span> a,b,c </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(x,y,z))

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">export_tikz</span></span><span>(<span class="hljs-params">
    centers, drift, U_grid, X1, X2,
    curl=</span></span><span><span class="hljs-literal">None</span></span><span>, attractors=</span><span><span class="hljs-literal">None</span></span><span>,
    filename=</span><span><span class="hljs-string">"tikz_rsvp_figures.tex"</span></span><span>
):
    <span class="hljs-string">"""
    centers: (N,2) latent coords sampled
    drift: (N,2) drift vectors at centers
    U_grid: 2D potential array matching mesh X1,X2
    X1,X2: meshgrid arrays
    curl: (N,) curl magnitudes (optional)
    attractors: (K,2) optional attractor points
    """</span>
    </span><span><span class="hljs-comment"># Flatten grid for surface plot</span></span><span>
    flat_x, flat_y, flat_u = X1.ravel(), X2.ravel(), U_grid.ravel()

    </span><span><span class="hljs-comment"># Potential surface</span></span><span>
    pot_surface = </span><span><span class="hljs-string">"\n"</span></span><span>.join(
        </span><span><span class="hljs-string">f"<span class="hljs-subst">{x:<span class="hljs-number">.5</span></span></span></span><span>f} </span><span><span class="hljs-subst">{y:<span class="hljs-number">.5</span></span></span><span>f} </span><span><span class="hljs-subst">{u:<span class="hljs-number">.5</span></span></span><span>f}"
        </span><span><span class="hljs-keyword">for</span></span><span> x, y, u </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(flat_x, flat_y, flat_u)
    )

    </span><span><span class="hljs-comment"># Drift vector field as quiver points</span></span><span>
    drift_coords = </span><span><span class="hljs-string">"\n"</span></span><span>.join(
        </span><span><span class="hljs-string">f"<span class="hljs-subst">{x:<span class="hljs-number">.5</span></span></span></span><span>f} </span><span><span class="hljs-subst">{y:<span class="hljs-number">.5</span></span></span><span>f} </span><span><span class="hljs-subst">{dx:<span class="hljs-number">.5</span></span></span><span>f} </span><span><span class="hljs-subst">{dy:<span class="hljs-number">.5</span></span></span><span>f}"
        </span><span><span class="hljs-keyword">for</span></span><span> (x,y), (dx,dy) </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(centers, drift)
    )

    </span><span><span class="hljs-comment"># Curl scatter if available</span></span><span>
    curl_coords = </span><span><span class="hljs-string">""</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> curl </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
        curl_coords = </span><span><span class="hljs-string">"\n"</span></span><span>.join(
            </span><span><span class="hljs-string">f"<span class="hljs-subst">{x:<span class="hljs-number">.5</span></span></span></span><span>f} </span><span><span class="hljs-subst">{y:<span class="hljs-number">.5</span></span></span><span>f} </span><span><span class="hljs-subst">{c:<span class="hljs-number">.5</span></span></span><span>f}"
            </span><span><span class="hljs-keyword">for</span></span><span> (x,y), c </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(centers, curl)
        )

    </span><span><span class="hljs-comment"># Attractors</span></span><span>
    attractor_coords = </span><span><span class="hljs-string">""</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> attractors </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
        attractor_coords = to_latex_coords(attractors[:,</span><span><span class="hljs-number">0</span></span><span>], attractors[:,</span><span><span class="hljs-number">1</span></span><span>])

    latex = <span class="hljs-string">f"""
% Auto-generated TikZ plots from RSVP analysis
% Copy into your LaTeX document (requires pgfplots, 3d view)

%% ---- Potential U(x) surface data ----
\\def\\PotentialSurface{{
</span></span><span><span class="hljs-subst">{pot_surface}</span></span><span>
}}

%% ---- Drift field vectors (x, y, dx, dy) ----
\\def\\DriftField{{
</span><span><span class="hljs-subst">{drift_coords}</span></span><span>
}}

%% ---- Curl magnitudes (x, y, |curl|) ----
\\def\\CurlField{{
</span><span><span class="hljs-subst">{curl_coords}</span></span><span>
}}

%% ---- Attractor points ----
\\def\\AttractorCoords{{ </span><span><span class="hljs-subst">{attractor_coords}</span></span><span> }}

% ================= FIGURE: POTENTIAL LANDSCAPE =================
\\begin{{figure}}[H]
\\centering
\\begin{{tikzpicture}}
\\begin{{axis}}[
    width=0.85\\linewidth,
    view={{0}}{{90}},
    colormap/viridis,
    xlabel={{Latent $x_1$}},
    ylabel={{Latent $x_2$}},
    title={{Reconstructed Cognitive Potential $U(x)$}}
]
\\addplot3[
    surf,
    shader=interp,
] table[header=false]{{\\PotentialSurface}};
\\end{{axis}}
\\end{{tikzpicture}}
\\caption{{Latent potential reconstructed from sampled drift.}}
\\end{{figure}}

% ================= FIGURE: DRIFT FIELD =================
\\begin{{figure}}[H]
\\centering
\\begin{{tikzpicture}}
\\begin{{axis}}[
    width=0.85\\linewidth,
    xlabel={{$x_1$}},
    ylabel={{$x_2$}},
    title={{Drift Field $f(x) \\approx -\\nabla U(x)$}}
]
\\addplot[quiver={{
    u={{\\thisrow{{2}}}},
    v={{\\thisrow{{3}}}},
    scale arrows=0.2
}}, -stealth] table[header=false]{{\\DriftField}};
\\end{{axis}}
\\end{{tikzpicture}}
\\caption{{Estimated drift vectors in latent space.}}
\\end{{figure}}

% ================= FIGURE: CURL TEST =================
\\begin{{figure}}[H]
\\centering
\\begin{{tikzpicture}}
\\begin{{axis}}[
    width=0.85\\linewidth,
    view={{0}}{{90}},
    colormap/hot,
    colorbar,
    title={{|curl f(x)| (Irrotationality Test)}}
]
\\addplot3[
    only marks,
    scatter,
    scatter src=explicit,
    mark=,
] table[x index=0, y index=1, meta index=2, header=false]{{\\CurlField}};
\\end{{axis}}
\\end{{tikzpicture}}
\\caption{{Drift field curl magnitudes; near-zero indicates conservative potential.}}
\\end{{figure}}

% ================= FIGURE: ATTRACTORS =================
\\begin{{figure}}[H]
\\centering
\\begin{{tikzpicture}}
\\begin{{axis}}[
    width=0.85\\linewidth,
    view={{0}}{{90}},
    colormap/viridis
]
\\addplot3[
    surf, opacity=0.5, shader=interp
] table[header=false]{{\\PotentialSurface}};

\\addplot3[only marks, mark=*, mark options={{fill=white, draw=black}}]
    coordinates {{ \\AttractorCoords }};

\\end{{axis}}
\\end{{tikzpicture}}
\\caption{{Recovered attractors (latent policy minima).}}
\\end{{figure}}
    """

    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(filename, </span><span><span class="hljs-string">"w"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
        f.write(latex)

    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"✅ TikZ export saved to <span class="hljs-subst">{filename}</span></span></span><span>")
</span></span></code></div></div></pre>
