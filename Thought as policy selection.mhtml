</span><span><span class="hljs-keyword">from</span></span><span> scipy.stats </span><span><span class="hljs-keyword">import</span></span><span> gini

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">l1_l2_ratio</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
    </span><span><span class="hljs-string">"""Sparsity = L1 / L2 (higher = sparser)"""</span></span><span>
    </span><span><span class="hljs-keyword">return</span></span><span> np.</span><span><span class="hljs-built_in">sum</span></span><span>(np.</span><span><span class="hljs-built_in">abs</span></span><span>(x)) / (np.linalg.norm(x) + </span><span><span class="hljs-number">1e-9</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">hoyer_sparsity</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
    </span><span><span class="hljs-string">"""Hoyer sparsity (0=dense, 1=sparse)"""</span></span><span>
    n = </span><span><span class="hljs-built_in">len</span></span><span>(x)
    l1 = np.</span><span><span class="hljs-built_in">sum</span></span><span>(np.</span><span><span class="hljs-built_in">abs</span></span><span>(x))
    l2 = np.sqrt(np.</span><span><span class="hljs-built_in">sum</span></span><span>(x**</span><span><span class="hljs-number">2</span></span><span>))
    </span><span><span class="hljs-keyword">return</span></span><span> (np.sqrt(n) - (l1 / (l2 + </span><span><span class="hljs-number">1e-9</span></span><span>))) / (np.sqrt(n) - </span><span><span class="hljs-number">1</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">neural_sparsity_metrics</span></span><span>(</span><span><span class="hljs-params">epoch_data</span></span><span>):
    <span class="hljs-string">"""
    epoch_data: shape (n_sensors, n_time)
    returns: sparsity over time for each metric
    """</span>
    l1l2, hoyer_vals, gini_vals = [], [], []
    </span><span><span class="hljs-keyword">for</span></span><span> t </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(epoch_data.shape[</span><span><span class="hljs-number">1</span></span><span>]):
        x = epoch_data[:, t]
        l1l2.append(l1_l2_ratio(x))
        hoyer_vals.append(hoyer_sparsity(x))
        gini_vals.append(gini(np.</span><span><span class="hljs-built_in">abs</span></span><span>(x)))
    </span><span><span class="hljs-keyword">return</span></span><span> {
        </span><span><span class="hljs-string">"l1_l2"</span></span><span>: np.array(l1l2),
        </span><span><span class="hljs-string">"hoyer"</span></span><span>: np.array(hoyer_vals),
        </span><span><span class="hljs-string">"gini"</span></span><span>: np.array(gini_vals)
    }
</span></span></code></div></div></pre>
