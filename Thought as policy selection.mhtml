</span><span><span class="hljs-keyword">from</span></span><span> scipy.optimize </span><span><span class="hljs-keyword">import</span></span><span> curve_fit

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">quad</span></span><span>(</span><span><span class="hljs-params">x, a, b, c</span></span><span>):
