#!/usr/bin/env python3
"""
CLI to regenerate figures and build PDF.
Usage:
    python regenerate.py [figures|3d|pdf|all]
"""
import sys, subprocess
cmd = sys.argv[1] if len(sys.argv)&gt;1 else "all"

def run(cmds):
    for c in cmds:
        print("RUN:", c)
        subprocess.check_call(c, shell=True)

