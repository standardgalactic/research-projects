</span><span><span class="hljs-keyword">from</span></span><span> scipy.optimize </span><span><span class="hljs-keyword">import</span></span><span> minimize

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">neg_log_likelihood</span></span><span>(</span><span><span class="hljs-params">beta, choices, means, stds, temp=<span class="hljs-number">1.0</span></span></span><span>):
    </span><span><span class="hljs-string">"""β = uncertainty bonus weight"""</span></span><span>
    beta = beta[</span><span><span class="hljs-number">0</span></span><span>]
    Q = means + beta * stds
    logits = Q / temp
    p = np.exp(logits) / np.</span><span><span class="hljs-built_in">sum</span></span><span>(np.exp(logits), axis=</span><span><span class="hljs-number">1</span></span><span>, keepdims=</span><span><span class="hljs-literal">True</span></span><span>)
    chosen = p[np.arange(</span><span><span class="hljs-built_in">len</span></span><span>(choices)), choices]
    </span><span><span class="hljs-keyword">return</span></span><span> -np.</span><span><span class="hljs-built_in">sum</span></span><span>(np.log(chosen + </span><span><span class="hljs-number">1e-9</span></span><span>))

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">fit_uncertainty_bonus</span></span><span>(</span><span><span class="hljs-params">choices, means, stds</span></span><span>):
    res = minimize(neg_log_likelihood, x0=[</span><span><span class="hljs-number">0.5</span></span><span>],
