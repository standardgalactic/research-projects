tree = cKDTree(centers)
curl_norms = []

</span><span><span class="hljs-keyword">for</span></span><span> i, x </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">enumerate</span></span><span>(centers):
    _, idx = tree.query(x, k=</span><span><span class="hljs-number">5</span></span><span>)
    neighbors = centers[idx]
    f_neigh = drift[idx]

    </span><span><span class="hljs-comment"># crude local curl estimate in 2D: ∂fy/∂x - ∂fx/∂y</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> d &gt;= </span><span><span class="hljs-number">2</span></span><span>:
        dx = neighbors[:,</span><span><span class="hljs-number">0</span></span><span>] - x[</span><span><span class="hljs-number">0</span></span><span>]
        dy = neighbors[:,</span><span><span class="hljs-number">1</span></span><span>] - x[</span><span><span class="hljs-number">1</span></span><span>]
        dfx = f_neigh[:,</span><span><span class="hljs-number">0</span></span><span>] - drift[i,</span><span><span class="hljs-number">0</span></span><span>]
        dfy = f_neigh[:,</span><span><span class="hljs-number">1</span></span><span>] - drift[i,</span><span><span class="hljs-number">1</span></span><span>]

        dfx_dy = np.polyfit(dy, dfx, </span><span><span class="hljs-number">1</span></span><span>)[</span><span><span class="hljs-number">0</span></span><span>] </span><span><span class="hljs-keyword">if</span></span><span> np.std(dy) &gt; </span><span><span class="hljs-number">1e-6</span></span><span> </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0</span></span><span>
        dfy_dx = np.polyfit(dx, dfy, </span><span><span class="hljs-number">1</span></span><span>)[</span><span><span class="hljs-number">0</span></span><span>] </span><span><span class="hljs-keyword">if</span></span><span> np.std(dx) &gt; </span><span><span class="hljs-number">1e-6</span></span><span> </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0</span></span><span>

        curl = dfy_dx - dfx_dy
