<span class="hljs-string">"""
MNE preprocessing pipeline for EEG/MEG -&gt; epoch matrices
Functions:
 - load_raw(file, fmt)    : wrap to load raw data (EDF, FIF, BDF, etc)
 - basic_preproc(raw)     : bandpass, notch, downsample, ICA (optional)
 - make_epochs(raw, events, tmin, tmax) : epoch the raw into numpy arrays (n_epochs, n_sensors, n_times)
 - epoch_to_feature_matrix(epochs, twin=None, step=None) : return per-epoch sensor x time matrices (or summarized windows)
"""</span>

</span><span><span class="hljs-keyword">import</span></span><span> mne
</span><span><span class="hljs-keyword">import</span></span><span> numpy </span><span><span class="hljs-keyword">as</span></span><span> np

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">load_raw</span></span><span>(</span><span><span class="hljs-params">path, fmt=<span class="hljs-string">"auto"</span></span></span><span>, preload=</span><span><span class="hljs-literal">True</span></span><span>):
    </span><span><span class="hljs-keyword">if</span></span><span> fmt == </span><span><span class="hljs-string">"fif"</span></span><span> </span><span><span class="hljs-keyword">or</span></span><span> path.endswith(</span><span><span class="hljs-string">".fif"</span></span><span>):
        raw = mne.io.read_raw_fif(path, preload=preload)
    </span><span><span class="hljs-keyword">elif</span></span><span> path.endswith(</span><span><span class="hljs-string">".edf"</span></span><span>):
        raw = mne.io.read_raw_edf(path, preload=preload)
    </span><span><span class="hljs-keyword">elif</span></span><span> path.endswith(</span><span><span class="hljs-string">".bdf"</span></span><span>):
        raw = mne.io.read_raw_bdf(path, preload=preload)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        </span><span><span class="hljs-comment"># fallback to MNE auto</span></span><span>
        raw = mne.io.read_raw(path, preload=preload)
    </span><span><span class="hljs-keyword">return</span></span><span> raw

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">basic_preproc</span></span><span>(</span><span><span class="hljs-params">raw, l_freq=<span class="hljs-number">1.0</span></span></span><span>, h_freq=</span><span><span class="hljs-number">40.0</span></span><span>, notch_freqs=(</span><span><span class="hljs-params"><span class="hljs-number">50</span></span></span><span>,</span><span><span class="hljs-number">60</span></span><span>), resample_sfreq=</span><span><span class="hljs-number">250</span></span><span>, do_ica=</span><span><span class="hljs-literal">True</span></span><span>):
    raw = raw.copy().</span><span><span class="hljs-built_in">filter</span></span><span>(l_freq, h_freq, fir_design=</span><span><span class="hljs-string">"firwin"</span></span><span>, verbose=</span><span><span class="hljs-literal">False</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> notch_freqs:
        raw.notch_filter(notch_freqs, verbose=</span><span><span class="hljs-literal">False</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> resample_sfreq:
        raw.resample(resample_sfreq, npad=</span><span><span class="hljs-string">"auto"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> do_ica:
        ica = mne.preprocessing.ICA(n_components=</span><span><span class="hljs-built_in">min</span></span><span>(</span><span><span class="hljs-number">20</span></span><span>, raw.info[</span><span><span class="hljs-string">'nchan'</span></span><span>]-</span><span><span class="hljs-number">1</span></span><span>), random_state=</span><span><span class="hljs-number">42</span></span><span>, verbose=</span><span><span class="hljs-literal">False</span></span><span>)
        ica.fit(raw)
        </span><span><span class="hljs-comment"># try to find EOG components automatically</span></span><span>
        eog_indices, eog_scores = ica.find_bads_eog(raw, ch_name=</span><span><span class="hljs-literal">None</span></span><span>)
        ica.exclude = eog_indices
        raw = ica.apply(raw.copy())
    </span><span><span class="hljs-keyword">return</span></span><span> raw

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">make_epochs</span></span><span>(</span><span><span class="hljs-params">raw, events, event_id, tmin=-<span class="hljs-number">0.2</span></span></span><span>, tmax=</span><span><span class="hljs-number">0.8</span></span><span>, baseline=(</span><span><span class="hljs-params"><span class="hljs-literal">None</span></span></span><span>,</span><span><span class="hljs-number">0</span></span><span>)):
    <span class="hljs-string">"""
    events: array of shape (n_events, 3) or MNE events
    event_id: dict or int
    returns: mne.Epochs object
    """</span>
    epochs = mne.Epochs(raw, events, event_id=event_id, tmin=tmin, tmax=tmax, baseline=baseline, preload=</span><span><span class="hljs-literal">True</span></span><span>, verbose=</span><span><span class="hljs-literal">False</span></span><span>)
    </span><span><span class="hljs-keyword">return</span></span><span> epochs

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">epoch_to_matrix</span></span><span>(</span><span><span class="hljs-params">epochs, picks=<span class="hljs-literal">None</span></span></span><span>):
    <span class="hljs-string">"""
    Convert epochs to numpy array shape (n_epochs, n_channels, n_times)
    """</span>
    </span><span><span class="hljs-keyword">if</span></span><span> picks </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
        data = epochs.get_data(picks=picks)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        data = epochs.get_data()
    </span><span><span class="hljs-keyword">return</span></span><span> data  </span><span><span class="hljs-comment"># (n_epochs, n_ch, n_times)</span></span><span>

</span><span><span class="hljs-comment"># Example usage:</span></span><span>
</span><span><span class="hljs-comment"># raw = load_raw("subject1_raw.fif")</span></span><span>
</span><span><span class="hljs-comment"># raw = basic_preproc(raw)</span></span><span>
</span><span><span class="hljs-comment"># events = mne.find_events(raw)  # or load from stim channel</span></span><span>
</span><span><span class="hljs-comment"># epochs = make_epochs(raw, events, event_id=1)</span></span><span>
</span><span><span class="hljs-comment"># X = epoch_to_matrix(epochs)</span></span><span>
