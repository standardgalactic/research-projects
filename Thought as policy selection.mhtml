</span><span><span class="hljs-comment"># Returns drift estimate f(x) and diffusion matrix D(x) evaluated at a set of grid centers</span></span><span>

</span><span><span class="hljs-keyword">from</span></span><span> sklearn.neighbors </span><span><span class="hljs-keyword">import</span></span><span> NearestNeighbors

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">multivariate_kramers_moyal</span></span><span>(</span><span><span class="hljs-params">Xlat, dt=<span class="hljs-number">0.01</span></span></span><span>, n_grid=</span><span><span class="hljs-number">200</span></span><span>, k_neighbors=</span><span><span class="hljs-number">200</span></span><span>):
    </span><span><span class="hljs-comment"># Xlat: (n_dim, T)</span></span><span>
    n_dim, T = Xlat.shape
    xs = Xlat.T  </span><span><span class="hljs-comment"># T x n_dim</span></span><span>
    </span><span><span class="hljs-comment"># sample grid points from data quantiles (subsample for speed)</span></span><span>
    inds = np.linspace(</span><span><span class="hljs-number">0</span></span><span>, T-</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-built_in">min</span></span><span>(n_grid, T)).astype(</span><span><span class="hljs-built_in">int</span></span><span>)
    grid = xs[inds]
    nn = NearestNeighbors(n_neighbors=k_neighbors, algorithm=</span><span><span class="hljs-string">'auto'</span></span><span>).fit(xs)
    drift_centers = []
    diffusion_centers = []
    </span><span><span class="hljs-keyword">for</span></span><span> g </span><span><span class="hljs-keyword">in</span></span><span> grid:
        dists, nbrs = nn.kneighbors(g.reshape(</span><span><span class="hljs-number">1</span></span><span>,-</span><span><span class="hljs-number">1</span></span><span>), return_distance=</span><span><span class="hljs-literal">True</span></span><span>)
        nbrs = nbrs.flatten()
        Xn = xs[nbrs]          </span><span><span class="hljs-comment"># k x n_dim</span></span><span>
        Xn_next = xs[nbrs+</span><span><span class="hljs-number">1</span></span><span>] </span><span><span class="hljs-keyword">if</span></span><span> np.</span><span><span class="hljs-built_in">max</span></span><span>(nbrs+</span><span><span class="hljs-number">1</span></span><span>) &lt; T </span><span><span class="hljs-keyword">else</span></span><span> xs[nbrs]  </span><span><span class="hljs-comment"># simple shift (safe guard)</span></span><span>
        </span><span><span class="hljs-comment"># compute increments</span></span><span>
        dX = Xn_next - Xn
        </span><span><span class="hljs-comment"># linear regression drift ≈ E[dX]/dt</span></span><span>
        f_est = np.mean(dX, axis=</span><span><span class="hljs-number">0</span></span><span>) / dt  </span><span><span class="hljs-comment"># (n_dim,)</span></span><span>
        </span><span><span class="hljs-comment"># diffusion ≈ cov(dX)/(2*dt)</span></span><span>
        cov = np.cov(dX, rowvar=</span><span><span class="hljs-literal">False</span></span><span>)
        D_est = cov / (</span><span><span class="hljs-number">2</span></span><span>*dt)
        drift_centers.append(f_est)
        diffusion_centers.append(D_est)
    </span><span><span class="hljs-keyword">return</span></span><span> np.array(grid), np.array(drift_centers), np.array(diffusion_centers)

