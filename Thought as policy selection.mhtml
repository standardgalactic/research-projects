</span><span><span class="hljs-keyword">from</span></span><span> scipy.interpolate </span><span><span class="hljs-keyword">import</span></span><span> RBFInterpolator
</span><span><span class="hljs-keyword">from</span></span><span> numpy.linalg </span><span><span class="hljs-keyword">import</span></span><span> lstsq

</span><span><span class="hljs-comment"># centers: (N, d)</span></span><span>
</span><span><span class="hljs-comment"># drift:   (N, d)  ≈  -∇U(x)</span></span><span>

