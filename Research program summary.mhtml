    {</span><span><span class="hljs-string">"name"</span></span><span>: </span><span><span class="hljs-string">"GPT-4o"</span></span><span>, </span><span><span class="hljs-string">"NMI"</span></span><span>: 0.27, </span><span><span class="hljs-string">"Ricci_mean"</span></span><span>: 0.38, </span><span><span class="hljs-string">"H1"</span></span><span>: 0.15, </span><span><span class="hljs-string">"Zipf_alpha"</span></span><span>: 1.05},
