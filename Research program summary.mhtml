  </span><span><span class="hljs-built_in">let</span></span><span> k2x=vx2, k2y=vy2, k2vx=ax2, k2vy=ay2;

  // k3
  </span><span><span class="hljs-built_in">let</span></span><span> x3 = x + 0.5*dt*k2x, y3 = y + 0.5*dt*k2y;
