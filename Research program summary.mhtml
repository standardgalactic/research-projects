\part{Theoretical Foundations}

\section{Core Definitions}
\begin{definition}[Autogenerative System]
A system $(X,\mathcal{T},\mu)$ is autogenerative </span><span><span class="hljs-keyword">if</span></span><span> $X$ is a structured space (manifold/graph/sheaf), $\mathcal{T}:X^*\!\to\!\Delta(X)$ maps sequences to continuation distributions determined solely by the learned structure on $X$, and no external rules are used beyond that topology.
\end{definition}

\begin{definition}[Non-Markovian Index]
For process $\{X_t\}$,
\[
\mathrm{NMI}_k(t)=\frac{I(X_{t+1};X_{1:t-k}\mid X_{t-k+1:t})}{H(X_{t+1}\mid X_{t-k+1:t})}.
\]
Essential non-Markovianity holds </span><span><span class="hljs-keyword">if</span></span><span> $\mathrm{NMI}_k(t)\not\to 0$ as </span><span><span class="hljs-variable">$k</span></span><span>\to\infty$.
\end{definition}

\begin{definition}[Trajectory Manifold]
Embed sequences into a path space $\Gamma$ with rough-path topology and a WassersteinFisher metric, $\gamma:[0,T]\!\to\!X$ with $\gamma(t)=\phi(x_{1:t})$.
\end{definition}

\section{Fundamental Conjectures}
\paragraph{Conjecture A (Language Non-Markovianity).}
No finite $k$ uniformly approximates </span><span><span class="hljs-variable">$P</span></span><span>(x_{t+1}\mid x_{1:t})$ within a semantic tolerance $\epsilon$ across all contexts.

\paragraph{Conjecture B (Topological Necessity).}
Positive </span><span><span class="hljs-variable">$H</span></span><span>^1$ </span><span><span class="hljs-keyword">in</span></span><span> a language sheaf signals obstructions to globally coherent generation.

\paragraph{Conjecture C (Universal Structure).}
With unbounded training data, natural language embeddings converge (up to isometry) to a universal geometry.

\paragraph{Conjecture D (Physics Extension).}
Conservation laws correspond to Noether symmetries of autogenerative trajectory spaces.
