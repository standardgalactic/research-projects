  </span><span><span class="hljs-built_in">let</span></span><span> x4 = x + dt*k3x, y4 = y + dt*k3y;
  </span><span><span class="hljs-built_in">let</span></span><span> vx4 = vx + dt*k3vx, vy4 = vy + dt*k3vy;
  </span><span><span class="hljs-built_in">let</span></span><span> [ax4, ay4] = accel(x4,y4,vx4,vy4,a);
  </span><span><span class="hljs-built_in">let</span></span><span> k4x=vx4, k4y=vy4, k4vx=ax4, k4vy=ay4;

