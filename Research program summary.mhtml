.teleprompter { overflow: visible; position: relative; }

.teleprompter-inner { align-content: flex-start; display: flex; flex-wrap: wrap; inset: 0px; position: absolute; }

.teleprompter-word { animation: fade-in var(--fade,.5s) cubic-bezier(.37,.55,.86,.88) forwards; opacity: 0; white-space: pre; }

