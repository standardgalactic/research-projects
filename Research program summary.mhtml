  </span><span><span class="hljs-built_in">let</span></span><span> [ax3, ay3] = accel(x3,y3,vx3,vy3,a);
  </span><span><span class="hljs-built_in">let</span></span><span> k3x=vx3, k3y=vy3, k3vx=ax3, k3vy=ay3;

