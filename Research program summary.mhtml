import json, numpy as np
from pathlib import Path

def integrate_geodesic(x0, y0, vx0, vy0, a=0.3, dt=0.02, steps=400):
    xs, ys = [x0], [y0]
    vx, vy, x, y = vx0, vy0, x0, y0
    </span><span><span class="hljs-keyword">for</span></span><span> _ </span><span><span class="hljs-keyword">in</span></span><span> range(steps):
        </span><span><span class="hljs-comment"># Christoffel symbols for metric g = diag(e^{2a x}, e^{-2a x})</span></span><span>
        Gx_xx = a
        Gx_yy = -a*np.exp(-4*a*x)
        Gy_xy = a
        </span><span><span class="hljs-comment"># geodesic eqns: d^2 x / dt^2 + ^x_{ij} v^i v^j = 0, same for y</span></span><span>
        ax = - (Gx_xx*vx*vx + Gx_yy*vy*vy)
        ay = -2*Gy_xy*vx*vy
        vx += ax*dt
        vy += ay*dt
        x += vx*dt
        y += vy*dt
        xs.append(x)
        ys.append(y)
    </span><span><span class="hljs-built_in">return</span></span><span> np.column_stack((xs, ys)).tolist()

def main():
    outdir = Path(</span><span><span class="hljs-string">"site/data"</span></span><span>); outdir.mkdir(parents=True, exist_ok=True)
    curves = []
    </span><span><span class="hljs-keyword">for</span></span><span> seed,(x0,y0,vx0,vy0) </span><span><span class="hljs-keyword">in</span></span><span> enumerate([(0,0,1,0.2),(0,-0.5,0.8,0.4),(0,0.5,1,-0.2)]):
        pts = integrate_geodesic(x0,y0,vx0,vy0)
        curves.append({</span><span><span class="hljs-string">"id"</span></span><span>: seed, </span><span><span class="hljs-string">"points"</span></span><span>: pts})
    with open(outdir/</span><span><span class="hljs-string">"geodesics.json"</span></span><span>,</span><span><span class="hljs-string">"w"</span></span><span>) as f:
        json.dump(curves, f)
    </span><span><span class="hljs-built_in">print</span></span><span>(f</span><span><span class="hljs-string">"[OK] Wrote {len(curves)} synthetic geodesics -&gt; {outdir/'geodesics.json'}"</span></span><span>)

</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    main()
