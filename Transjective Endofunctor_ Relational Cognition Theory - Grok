<span class="line"><span style="color: rgb(106, 153, 85);">%======================================================================</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(197, 134, 192);">\documentclass</span><span style="color: rgb(230, 230, 230);">[</span><span style="color: rgb(156, 220, 254);">12pt,a4paper,twoside</span><span style="color: rgb(230, 230, 230);">]{</span><span style="color: rgb(78, 201, 176);">book</span><span style="color: rgb(230, 230, 230);">}</span></span>
