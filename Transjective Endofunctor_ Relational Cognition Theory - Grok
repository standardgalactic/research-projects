<span class="line"><span style="color: rgb(197, 134, 192);">\label</span><span style="color: rgb(230, 230, 230);">{</span><span style="color: rgb(156, 220, 254);">sec:8</span><span style="color: rgb(230, 230, 230);">}</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\begin</span><span style="color: rgb(230, 230, 230);">{</span><span style="color: rgb(156, 220, 254);">theorem</span><span style="color: rgb(230, 230, 230);">}[Equilibrium Law]</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">\[</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">\nabla</span><span style="color: rgb(78, 201, 176);"> R=</span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(220, 220, 170);">\;</span><span style="color: rgb(86, 156, 214);">\Longleftrightarrow</span><span style="color: rgb(220, 220, 170);">\;</span><span style="color: rgb(78, 201, 176);">T^</span><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(78, 201, 176);">=T</span><span style="color: rgb(220, 220, 170);">\;</span><span style="color: rgb(86, 156, 214);">\Longleftrightarrow</span><span style="color: rgb(220, 220, 170);">\;</span><span style="color: rgb(78, 201, 176);">\dot W=</span><span style="color: rgb(181, 206, 168);">0</span><span style="color: rgb(78, 201, 176);">.</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">\]</span></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\end</span><span style="color: rgb(230, 230, 230);">{</span><span style="color: rgb(156, 220, 254);">theorem</span><span style="color: rgb(230, 230, 230);">}</span></span>
