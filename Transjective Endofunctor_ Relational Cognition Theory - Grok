<span class="line"><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(78, 201, 176);">i</span><span style="color: rgb(86, 156, 214);">\hbar</span><span style="color: rgb(78, 201, 176);">_{\mathrm{eff}}</span><span style="color: rgb(86, 156, 214);">\Delta</span><span style="color: rgb(78, 201, 176);">\mathcal{S}_{\mathrm{QCE}}.</span></span>
