<span class="line"><span style="color: rgb(220, 220, 170);">\begin</span><span style="color: rgb(78, 201, 176);">{</span><span style="color: rgb(156, 220, 254);">cases</span><span style="color: rgb(78, 201, 176);">}</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(78, 201, 176);">_m </span><span style="color: rgb(197, 134, 192);">&amp;</span><span style="color: rgb(78, 201, 176);"> E(</span><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(78, 201, 176);">_m) \le \max(E(</span><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(78, 201, 176);">_a), E(</span><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(78, 201, 176);">_b)) + </span><span style="color: rgb(86, 156, 214);">\epsilon</span><span style="color: rgb(78, 201, 176);">_{\mathrm{merge}} </span><span style="color: rgb(197, 134, 192);">\\</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">\bot</span><span style="color: rgb(197, 134, 192);"> &amp;</span><span style="color: rgb(78, 201, 176);"> \text{otherwise}</span></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\end</span><span style="color: rgb(78, 201, 176);">{</span><span style="color: rgb(156, 220, 254);">cases</span><span style="color: rgb(78, 201, 176);">}</span></span>
