<span class="line"><span style="color: rgb(86, 156, 214);">    \item</span><span style="color: rgb(230, 230, 230);"> Idempotence: </span><span style="color: rgb(78, 201, 176);">$</span><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(86, 156, 214);"> \oplus</span><span style="color: rgb(86, 156, 214);"> \sigma</span><span style="color: rgb(78, 201, 176);"> = </span><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(78, 201, 176);">$</span><span style="color: rgb(230, 230, 230);">,</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">    \item</span><span style="color: rgb(230, 230, 230);"> Commutativity: </span><span style="color: rgb(78, 201, 176);">$</span><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(78, 201, 176);">_a </span><span style="color: rgb(86, 156, 214);">\oplus</span><span style="color: rgb(86, 156, 214);"> \sigma</span><span style="color: rgb(78, 201, 176);">_b = </span><span style="color: rgb(86, 156, 214);">\sigma</span><span style="color: rgb(78, 201, 176);">_b </span><span style="color: rgb(86, 156, 214);">\oplus</span><span style="color: rgb(86, 156, 214);"> \sigma</span><span style="color: rgb(78, 201, 176);">_a</span><span style="color: rgb(78, 201, 176);">$</span><span style="color: rgb(230, 230, 230);"> (when defined),</span></span>
