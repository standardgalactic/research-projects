<span class="line"><span style="color: rgb(78, 201, 176);">\subsection{Recursive Descent Parser (Python)}</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\begin</span><span style="color: rgb(78, 201, 176);">{</span><span style="color: rgb(156, 220, 254);">lstlisting</span><span style="color: rgb(78, 201, 176);">}[language=python]</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">class Parser:</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">    def __init__(self, tokens): self.tokens = tokens; self.pos = 0</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">    def parse(self):</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        stmts = []</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        while self.pos &lt; len(self.tokens):</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">            stmts.append(self.parse_stmt())</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        return Program(stmts)</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">    def parse_stmt(self):</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        tok = self.peek()</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        if tok == "sphere": return self.parse_make()</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        if tok == "pop": return self.parse_pop()</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        if tok == "merge": return self.parse_merge()</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        if tok == "close": return self.parse_close()</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        if tok == "rule": return self.parse_rule()</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        raise SyntaxError()</span></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\end</span><span style="color: rgb(78, 201, 176);">{</span><span style="color: rgb(156, 220, 254);">lstlisting</span><span style="color: rgb(78, 201, 176);">}</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(78, 201, 176);">\subsection{Runtime Interpreter}</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\begin</span><span style="color: rgb(78, 201, 176);">{</span><span style="color: rgb(156, 220, 254);">lstlisting</span><span style="color: rgb(78, 201, 176);">}[language=python]</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">class Interpreter:</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">    def __init__(self): self.spheres = {}; self.rules = {}</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">    def exec(self, stmt):</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">        match stmt:</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">            case MakeSphere(id, T, M): </span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">                sigma = Sphere(id, T, M, entropy(M), [])</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">                self.spheres[id] = sigma</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">            case Pop(src, dst, chain):</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">                sigma = self.spheres[src]</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">                for r in chain: sigma = self.apply_rule(sigma, r)</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">                self.spheres[dst] = sigma</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">            case Merge(a, b, stake):</span></span>
