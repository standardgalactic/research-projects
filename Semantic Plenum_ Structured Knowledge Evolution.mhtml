<span class="line"><span style="color: rgb(86, 156, 214);">    \item</span><span style="color: rgb(230, 230, 230);"> Entropy checking: </span><span style="color: rgb(78, 201, 176);">\(</span><span style="color: rgb(78, 201, 176);">O(</span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(78, 201, 176);">)</span><span style="color: rgb(78, 201, 176);">\)</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">    \item</span><span style="color: rgb(230, 230, 230);"> Merkle-style proof verification: </span><span style="color: rgb(78, 201, 176);">\(</span><span style="color: rgb(78, 201, 176);">O(\log n)</span><span style="color: rgb(78, 201, 176);">\)</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">    \item</span><span style="color: rgb(230, 230, 230);"> Closure synthesis: </span><span style="color: rgb(78, 201, 176);">\(</span><span style="color: rgb(78, 201, 176);">O(m </span><span style="color: rgb(86, 156, 214);">\cdot</span><span style="color: rgb(78, 201, 176);"> C_{\text{synth}})</span><span style="color: rgb(78, 201, 176);">\)</span></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\end</span><span style="color: rgb(230, 230, 230);">{</span><span style="color: rgb(156, 220, 254);">itemize</span><span style="color: rgb(230, 230, 230);">}</span></span>
