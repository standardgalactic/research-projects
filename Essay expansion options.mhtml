\subsection{Mathematical Preliminaries}
\label{subsec:math-prelim}

\begin{assumption}
\label{ass:manifold}
Let \(\mathbb{M}\) be a compact, orientable, 3-dimensional Riemannian manifold without boundary, equipped with metric tensor \(g_{\mu\nu}\) and Levi-Civita connection \(\Gamma^\lambda_{\mu\nu}\). The function spaces are Sobolev \(W^{2,2}(\mathbb{M})\) for \(\Phi, S\) and \(W^{1,2}(\mathbb{M}; T\mathbb{M})\) for \(\boldsymbol{\mathcal{v}}\).
\end{assumption}

The RSVP fields are defined as:
\begin{align}
\Phi &amp;: \mathbb{M} \to \mathbb{R}, \quad \Phi \in C^\infty(\mathbb{M}), \\
\boldsymbol{\mathcal{v}} &amp;: \mathbb{M} \to T\mathbb{M}, \quad \boldsymbol{\mathcal{v}} \in \Gamma(T\mathbb{M}), \\
S &amp;: \mathbb{M} \to \mathbb{R}_{\geq 0}, \quad S \in C^\infty(\mathbb{M}).
\end{align}

The core evolution equations are:
\begin{align}
\partial_t \Phi &amp;= -\nabla \cdot (\Phi \boldsymbol{\mathcal{v}}) + \xi(t), \label{eq:phi} \\
\partial_t \boldsymbol{\mathcal{v}} &amp;= (\nabla \Phi) \times \boldsymbol{\mathcal{v}} - \gamma \boldsymbol{\mathcal{v}} + \nabla \cdot \mathsf{D} \nabla \boldsymbol{\mathcal{v}} + \boldsymbol{\eta}(t), \label{eq:v} \\
\partial_t S &amp;= -\boldsymbol{\mathcal{v}} \cdot \nabla S + \alpha \|\nabla \Phi\|^2 + \beta \|\nabla \times \boldsymbol{\mathcal{v}}\|^2 + \kappa \Delta S. \label{eq:s}
\end{align}

\begin{theorem}[Local Existence and Uniqueness]
Under Assumption \ref{ass:manifold} and Lipschitz continuity of \(\xi, \boldsymbol{\eta}\), there exists \(T &gt; 0\) and a unique solution \((\Phi, \boldsymbol{\mathcal{v}}, S) \in C([0,T]; W^{2,2} \times W^{1,2} \times W^{2,2})\) to \eqref{eq:phi}--\eqref{eq:s}.
\end{theorem}

\begin{proof}
Apply the Picard--Lindelöf theorem in the appropriate Banach space; details follow standard semilinear PDE theory.
\end{proof}

