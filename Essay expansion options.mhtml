</span><span><span class="hljs-keyword">\int</span></span><span><span class="hljs-built_in">_</span></span><span>M </span><span><span class="hljs-keyword">\!</span></span><span> </span><span><span class="hljs-keyword">\Phi</span></span><span><span class="hljs-keyword">\,</span></span><span> R</span><span><span class="hljs-built_in">_</span></span><span>{ij}</span><span><span class="hljs-keyword">\,</span></span><span>dV = 0,
</span><span><span class="hljs-keyword">\]</span></span><span>
