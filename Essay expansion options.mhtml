</span><span><span class="hljs-keyword">\chi</span></span><span><span class="hljs-built_in">_</span></span><span>{ij} = </span><span><span class="hljs-keyword">\exp</span></span><span>(i(</span><span><span class="hljs-keyword">\theta</span></span><span><span class="hljs-built_in">_</span></span><span>i-</span><span><span class="hljs-keyword">\theta</span></span><span><span class="hljs-built_in">_</span></span><span>j)).
