<span class="line"><span style="color: rgb(106, 153, 85);">// -------- Subtitles Sync with Audio (CORRECTED &amp; ROBUST) --------</span></span>
<span class="line"><span style="color: rgb(86, 156, 214);">const</span><span style="color: rgb(156, 220, 254);"> player</span><span style="color: rgb(212, 212, 212);"> = </span><span style="color: rgb(156, 220, 254);">document</span><span style="color: rgb(212, 212, 212);">.</span><span style="color: rgb(220, 220, 170);">getElementById</span><span style="color: rgb(212, 212, 212);">(</span><span style="color: rgb(206, 145, 120);">'player'</span><span style="color: rgb(212, 212, 212);">);</span></span>
