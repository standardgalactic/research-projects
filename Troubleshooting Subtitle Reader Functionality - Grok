&lt;script&gt;
// -------- Load and Display Markdown Articles --------
const articleLinks = document.querySelectorAll('.article-link');
const articleList = document.getElementById('articleList');
const articleContent = document.getElementById('articleContent');
const backButton = document.getElementById('backButton');
articleLinks.forEach(link =&gt; {
&amp;nbsp;&amp;nbsp;link.addEventListener('click', async e =&gt; {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;e.preventDefault();
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const file = link.getAttribute('data-file');
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const res = await fetch(file);
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const text = await res.text();
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;articleList.style.display = 'none';
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;backButton.style.display = 'inline-block';
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;articleContent.style.display = 'block';
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;articleContent.innerHTML = marked.parse(text);
&amp;nbsp;&amp;nbsp;});
});
function showList() {
&amp;nbsp;&amp;nbsp;articleContent.style.display = 'none';
&amp;nbsp;&amp;nbsp;backButton.style.display = 'none';
&amp;nbsp;&amp;nbsp;articleList.style.display = 'block';
}
&lt;script&gt;
// -------- Subtitles Sync with Audio (improved) --------
const player = document.getElementById('player');
const subtitleDiv = document.getElementById('subtitle');
let subtitles = [];
let currentSub = -1;
// try VTT first, then SRT
loadSubtitles('background/Geometric_Consciousness_and_LLMs.vtt')
&amp;nbsp;&amp;nbsp;.catch(() =&gt; loadSubtitles('background/Geometric_Consciousness_and_LLMs.srt'))
&amp;nbsp;&amp;nbsp;.then(ok =&gt; {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;if (ok) subtitleDiv.textContent = "[Subtitles loaded ‚Äî press play]";
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;else subtitleDiv.textContent = "[No subtitles found]";
&amp;nbsp;&amp;nbsp;});
async function loadSubtitles(path) {
&amp;nbsp;&amp;nbsp;try {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const res = await fetch(path);
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;if (!res.ok) return false;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const text = await res.text();
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;parseSubtitles(text);
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;return true;
&amp;nbsp;&amp;nbsp;} catch {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;return false;
&amp;nbsp;&amp;nbsp;}
}
function parseSubtitles(data) {
&amp;nbsp;&amp;nbsp;// detect format
&amp;nbsp;&amp;nbsp;if (data.startsWith("WEBVTT")) {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;// WebVTT
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const pattern = /(\d{2}:\d{2}:\d{2}\.\d{3}) --&gt; (\d{2}:\d{2}:\d{2}\.\d{3})[\s\S]*?(?=\n\n|$)/g;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;let match;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;while ((match = pattern.exec(data)) !== null) {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const lines = match[0].split('\n').slice(1).join(' ');
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const [start, end] = match[1] ? [match[1], match[2]] : [];
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;subtitles.push({
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;start: toSecondsVTT(match[1]),
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;end: toSecondsVTT(match[2]),
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;text: lines.trim()
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;});
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}
&amp;nbsp;&amp;nbsp;} else {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;// SRT
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const pattern = /\d+\s*\n(\d{2}:\d{2}:\d{2}),(\d{3}) --&gt; (\d{2}:\d{2}:\d{2}),(\d{3})\s*\n([\s\S]*?)(?=\n\n|$)/g;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;let match;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;while ((match = pattern.exec(data)) !== null) {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const start = `${match[1]}.${match[2]}`;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const end = `${match[3]}.${match[4]}`;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;subtitles.push({
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;start: toSecondsVTT(start),
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;end: toSecondsVTT(end),
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;text: match[5].replace(/\n/g, ' ').trim()
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;});
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}
&amp;nbsp;&amp;nbsp;}
}
function toSecondsVTT(t) {
&amp;nbsp;&amp;nbsp;const [h, m, s] = t.split(':');
&amp;nbsp;&amp;nbsp;return parseFloat(h) * 3600 + parseFloat(m) * 60 + parseFloat(s);
}
player.addEventListener('timeupdate', () =&gt; {
&amp;nbsp;&amp;nbsp;const t = player.currentTime;
&amp;nbsp;&amp;nbsp;if (subtitles.length === 0) return;
&amp;nbsp;&amp;nbsp;// keep currentSub if still in range
&amp;nbsp;&amp;nbsp;if (currentSub &gt;= 0 &amp;&amp; t &gt;= subtitles[currentSub].start &amp;&amp; t &lt;= subtitles[currentSub].end) return;
&amp;nbsp;&amp;nbsp;// binary search to find current subtitle
&amp;nbsp;&amp;nbsp;let lo = 0, hi = subtitles.length - 1;
&amp;nbsp;&amp;nbsp;while (lo &lt;= hi) {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;const mid = (lo + hi) &gt;&gt; 1;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;if (t &lt; subtitles[mid].start) hi = mid - 1;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;else if (t &gt; subtitles[mid].end) lo = mid + 1;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;else {
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;currentSub = mid;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;subtitleDiv.textContent = subtitles[mid].text;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;return;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}
&amp;nbsp;&amp;nbsp;}
&amp;nbsp;&amp;nbsp;currentSub = -1;
&amp;nbsp;&amp;nbsp;subtitleDiv.textContent = '';
});
&lt;/script&gt;
&lt;!-- Load marked.js for Markdown rendering --&gt;
&lt;script src="https://cdn.jsdelivr.net/npm/marked/marked.min.js"&gt;&lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</div><section class="inline-media-container flex flex-col gap-1 clear-both"></section><section class="auth-notification flex flex-col gap-1"></section></div></div><div class="order-first sticky hidden @md/mainview:block top-11 h-0 opacity-0 group-focus-within:opacity-100 group-hover:opacity-100 -mr-[36px] @lg/mainview:-mr-12"><div class="min-h-7 py-4 flex flex-col gap-0.5"><button class="inline-flex items-center justify-center gap-2 whitespace-nowrap text-sm font-medium leading-[normal] cursor-pointer focus-visible:outline-none focus-visible:ring-1 focus-visible:ring-ring disabled:opacity-60 disabled:cursor-not-allowed transition-colors duration-100 [&amp;_svg]:shrink-0 select-none text-fg-secondary hover:text-fg-primary hover:bg-button-ghost-hover disabled:hover:text-fg-secondary disabled:hover:bg-transparent [&amp;_svg]:hover:text-fg-primary h-10 w-10 rounded-full" type="button" aria-label="Editar" data-state="closed"><span style="opacity: 1; transform: none;"><svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="stroke-[2] size-4"><path d="M18.25 5.75C16.8693 4.36929 14.6307 4.36929 13.25 5.75L10.125 8.875L5.52404 13.476C4.86236 14.1376 4.45361 15.0104 4.36889 15.9423L4 20.0001L8.0578 19.6311C8.98967 19.5464 9.86234 19.1377 10.524 18.476L18.25 10.75C19.6307 9.36929 19.6307 7.13071 18.25 5.75V5.75Z" stroke="currentColor"></path><path d="M12.5 7.5L16.5 11.5" stroke="currentColor"></path></svg></span></button><button class="inline-flex items-center justify-center gap-2 whitespace-nowrap text-sm font-medium leading-[normal] cursor-pointer focus-visible:outline-none focus-visible:ring-1 focus-visible:ring-ring disabled:cursor-not-allowed transition-colors duration-100 [&amp;_svg]:shrink-0 select-none text-fg-secondary hover:text-fg-primary hover:bg-button-ghost-hover disabled:hover:text-fg-secondary disabled:hover:bg-transparent [&amp;_svg]:hover:text-fg-primary h-10 w-10 rounded-full opacity-0 group-focus-