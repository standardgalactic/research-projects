	public void run() {
		long lastTime = System.nanoTime();
		while (isRunning) {
			long now = System.nanoTime();
			float deltaTime = (now - lastTime) / 1_000_000_000.0f; // Convert nano to seconds
			lastTime = now;

			// 1. INPUT PROCESSING
